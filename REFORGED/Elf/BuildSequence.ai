//============================================================================
//  global builds
//============================================================================
function global_init_strategy takes nothing returns nothing
    call SetTierBlock(1, 0.75, 60, true)
    call SetTierBlock(2, 0.75, 90, true)
    if not IsMapFlagSet(MAP_RANDOM_HERO) then
      call AddBlock(1, uELF_ALTAR, false, 0, uANCIENT_WAR, 16)
      call AddBlock(1, uELF_ALTAR, true, 0, uDEN_OF_WONDERS, 36)
      call AddBlock(1, uELF_ALTAR, false, 0, uMOON_WELL, 16)
    endif
    call AddBlock(1, uANCIENT_WAR, true, 0, uANCIENT_PROTECT, 36)
    call AddBlock(1, uANCIENT_WAR, false, 0, uHUNTERS_HALL, 20)
    call AddBlock(1, uANCIENT_WAR, true, 0, uDEN_OF_WONDERS, 36)
    call AddHarass(1, 2, uHUNTRESS)
    call AddHarass(3, 2, uFAERIE_DRAGON)
    call AddHarass(4, 1, uDEMON_HUNTER)


















































endfunction
function global_build_sequence takes nothing returns nothing
    if GetGold() > 800 then
      call BuildFront(2, uANCIENT_PROTECT, 35)
    endif
    if GetRushFactor() < 5000 or GetPlayerRace(nearest_enemy) == RACE_UNDEAD then
      call BuildFront(2, uANCIENT_PROTECT, 51)
    endif
    if HasExpanded() then
      call DefendTownsFrontDone(2, uANCIENT_PROTECT, 75)
      if GetGold() > 2000 then
            call DefendTownsFrontDone(3, uANCIENT_PROTECT, 35)
            if tier >= 2 and TownCount(uANCIENT_PROTECT) >= 4 and TownCount(uDEN_OF_WONDERS) < 2 then
                  call DefendTownsDone(1, uDEN_OF_WONDERS, 40)
            endif
            if TownCount(uMOON_WELL) >= 8 then
                  call DefendTownsDone(2, uMOON_WELL, 35)
            endif    
      endif
    endif

    // Global Items
    call BuildItem(1, uM_SCROLL_OF_HEALING, 40)
    //call BuildItem(1, uSCROLL_OF_PROTECTION, 30)
    call BuildItem(1, uPOTION_OF_LESSER_INVULNERBILITY, 70)
    call BuildItem(1, uM_TOWN_PORTAL, 100)
    call BuildItem(1, uMOONSTONE, 45)
    if tier >= 2 then
      call BuildItem(1, uHEALING_POTION, 80)
      call BuildItem(1, uTOWN_PORTAL, 180)
      call BuildItem(1, uLESSER_CLARITY_POTION, 40)
      call BuildItem(1, uSTAFF_OF_PRESERVATION, 80)
    endif
    if tier >= 3 then
          if c_ally_total > 0 then
            call BuildItem(1, uSTAFF_OF_SANCTUARY, 30)
          endif
        //call BuildItem(1, uORB_OF_VENOM, 70)
        //call BuildItem(2, uM_SCROLL_OF_HEALING, 30)
    endif
    if hero[1] == uWARDEN or hero[1] == uDEMON_HUNTER then
      call BuildItem(1, uBOOTS_OF_SPEED, 50)
    endif
    if GetGold() > 1000 then
      call BuildItem(1, uSTAFF_OF_TELEPORTATION, 35)
    endif
    // DYNAMIC
    call ResetDynamicSystem()
    if tier == 1 then
      call AddUnitToAntiair(uARCHER,29)
      call AddUnitToAnticasters(uARCHER,29)
      call AddUnitToAntitowers(uBALLISTA,29)
      call AddUnitToAntipiercing(uHUNTRESS, 15)
      call AddUnitToAntipiercing(uARCHER,15)
      call AddUnitToAntinormal(uARCHER,20)
      call AddUnitToAntinormal(uHUNTRESS,10)
      call AddUnitToAntisiege(uARCHER,29)
      call AddUnitToAntimagic(uARCHER,29)
      call AddUnitToAntiunarmored(uARCHER,29)
      call AddUnitToAntilightarmor(uARCHER,29)
      call AddUnitToAntimediumarmor(uHUNTRESS,29)
      call AddUnitToAntiheavyarmor(uHUNTRESS,29)
      call DynamicBuildUnit(33, 38)
      call BuildUnit(1, merc_range, 65)
      call BuildUnit(1, merc_stopper, 65)
    elseif tier == 2 then
      call AddUnitToAntiair(uARCHER,15)
      call AddUnitToAntiair(uHIPPO,10)
      call AddUnitToAntiair(uDRUID_TALON,1)
      call AddUnitToAntiair(uDRYAD,10)
      call AddUnitToAnticasters(uFAERIE_DRAGON,2)
      call AddUnitToAnticasters(uARCHER,1)
      call AddUnitToAnticasters(uMOUNTAIN_GIANT,2)
      call AddUnitToAnticasters(uDRYAD,8+GetUpgradeLevel(old_id[uUPG_ABOLISH]))
      call AddUnitToAntitowers(uBALLISTA,4)
      call AddUnitToAntitowers(uMOUNTAIN_GIANT,1)
      call AddUnitToAntipiercing(uARCHER,5)
      call AddUnitToAntipiercing(uMOUNTAIN_GIANT,2)
      call AddUnitToAntinormal(uARCHER, 1)
      call AddUnitToAntinormal(uHUNTRESS,3)
      call AddUnitToAntinormal(uMOUNTAIN_GIANT,1)
      call AddUnitToAntinormal(uDRUID_CLAW,5)
      call AddUnitToAntisiege(uARCHER,1)
      call AddUnitToAntisiege(uMOUNTAIN_GIANT,1)
      call AddUnitToAntimagic(uARCHER,1)
      call AddUnitToAntimagic(uMOUNTAIN_GIANT,1)
      call AddUnitToAntimagic(uDRYAD,3)
      call AddUnitToAntimagic(uFAERIE_DRAGON,1)
      call AddUnitToAntiunarmored(uARCHER,7)
      call AddUnitToAntiunarmored(uDRYAD,7)
      call AddUnitToAntiunarmored(uHIPPO_RIDER,1)
      call AddUnitToAntiunarmored(uBALLISTA,1)
      call AddUnitToAntilightarmor(uARCHER,4)
      call AddUnitToAntilightarmor(uDRYAD,4)
      call AddUnitToAntilightarmor(uHIPPO_RIDER,1)
      call AddUnitToAntimediumarmor(uHUNTRESS,7)
      call AddUnitToAntimediumarmor(uDRUID_CLAW,4)
      call AddUnitToAntiheavyarmor(uDRUID_TALON,1)
      call AddUnitToAntiheavyarmor(uDRUID_CLAW,5)
      call DynamicBuildUnit(64, 40)
      call BuildUnit(1, dragons[3], 65)
      call BuildUnit(1, merc_range, 65)
    else
      call AddUnitToAntiair(uARCHER,15)
      call AddUnitToAntiair(uHIPPO,10)
      call AddUnitToAntiair(uDRUID_TALON,1)
      call AddUnitToAntiair(uDRYAD,10)
      call AddUnitToAnticasters(uFAERIE_DRAGON,2)
      call AddUnitToAnticasters(uARCHER,1)
      call AddUnitToAnticasters(uMOUNTAIN_GIANT,2)
      call AddUnitToAnticasters(uDRYAD,9)
      call AddUnitToAntitowers(uBALLISTA,5)
      call AddUnitToAntitowers(uMOUNTAIN_GIANT,2)
      call AddUnitToAntitowers(uCHIMAERA,GetUpgradeLevel(old_id[uUPG_CHIM_ACID]))
      call AddUnitToAntipiercing(uDRUID_CLAW,3)
      call AddUnitToAntipiercing(uARCHER,5)
      call AddUnitToAntipiercing(uMOUNTAIN_GIANT,2)
      call AddUnitToAntinormal(uHUNTRESS,1)
      call AddUnitToAntinormal(uMOUNTAIN_GIANT,1)
      call AddUnitToAntinormal(uDRUID_CLAW,5)
      call AddUnitToAntisiege(uARCHER,1)
      call AddUnitToAntisiege(uMOUNTAIN_GIANT,1)
      call AddUnitToAntimagic(uARCHER,1)
      call AddUnitToAntimagic(uMOUNTAIN_GIANT,1)
      call AddUnitToAntimagic(uDRYAD,3)
      call AddUnitToAntimagic(uFAERIE_DRAGON,1)
      call AddUnitToAntiunarmored(uARCHER,4)
      call AddUnitToAntiunarmored(uDRYAD,4)
      call AddUnitToAntiunarmored(uHIPPO_RIDER,1)
      call AddUnitToAntiunarmored(uBALLISTA,1)
      call AddUnitToAntilightarmor(uARCHER,7)
      call AddUnitToAntilightarmor(uDRYAD,7)
      call AddUnitToAntilightarmor(uHIPPO_RIDER,1)
      call AddUnitToAntimediumarmor(uHUNTRESS,1)
      call AddUnitToAntimediumarmor(uDRUID_CLAW,8)
      call AddUnitToAntiheavyarmor(uDRUID_TALON,1)
      call AddUnitToAntiheavyarmor(uCHIMAERA,3)
      call AddUnitToAntiheavyarmor(uDRUID_CLAW,5)
      call DynamicBuildUnit(100, 40)
      call BuildUnit(1, dragons[3], 65)
    endif

    // TIER 1 UPGRADES
    //--------------------------------------------------------
    if tier >= 1 then
      call BuildUpgr(1, uUPG_BACKPACK_ELF, 10)
    endif
    call BuildAdvUpgr2(1, uUPG_ULTRAVISION, 1, TownCountDone(uARCHER) + TownCountDone(uHUNTRESS), 5, 10, 10)
    call BuildAdvUpgr2(1, uUPG_SCOUT, 1,TownCountDone(uHUNTRESS),1,10,15)
    call BuildAdvUpgr2(3, uUPG_STR_MOON, 1,TownCountDone(uARCHER) + TownCountDone(uHUNTRESS) + TownCountDone(uHIPPO_RIDER),5,15,40)
    call BuildAdvUpgr2(3, uUPG_MOON_ARMOR, 1,TownCountDone(uARCHER) + TownCountDone(uHUNTRESS) + TownCountDone(uHIPPO_RIDER),5,15,40)
    call BuildAdvUpgr2(3, uUPG_STR_WILD, 1,TownCountDone(uDRYAD) + TownCountDone(uDRUID_CLAW) + TownCountDone(uMOUNTAIN_GIANT) + TownCountDone(uDRUID_TALON) + TownCountDone(uCHIMAERA) + TownCountDone(uFAERIE_DRAGON),3,15,40 )
    call BuildAdvUpgr2(3, uUPG_HIDES, 1,TownCountDone(uDRYAD) + TownCountDone(uDRUID_CLAW) + TownCountDone(uMOUNTAIN_GIANT) + TownCountDone(uDRUID_TALON) + TownCountDone(uCHIMAERA) + TownCountDone(uFAERIE_DRAGON),3,15,40 )


    // TIER 2 UPGRADES
    //--------------------------------------------------------
    if tier >= 2 then
      call BuildUpgr(1, uUPG_BLESSING, 50)
    endif
    call BuildAdvUpgr2(1, uUPG_BOWS,2,TownCountDone(uARCHER),1,10,40)
    if enemy_magic < 10 or enemy_piercing > enemy_magic*2 or enemy_siege > enemy_magic*2 then // converts to heavy armor if upgraded so avoid if enemy type counters that armor
      call BuildAdvUpgr2(1, uUPG_GLAIVE, 2,TownCountDone(uHUNTRESS),0.5,15,70)
    endif
    call BuildAdvUpgr2(1, uUPG_BOLT, 2,TownCountDone(uBALLISTA),0.25,10,40)
    call BuildAdvUpgr2(1, uUPG_ABOLISH, 2, TownCountDone(uDRYAD),0.33,15,75)
    call BuildAdvUpgr2(2, uUPG_DRUID_CLAW, 2, TownCountDone(uDRUID_CLAW),0.33,15,80)
    call BuildAdvUpgr2(2, uUPG_DRUID_TALON, 2, TownCountDone(uDRUID_TALON),0.33,15,80)
    call BuildAdvUpgr2(1, uUPG_MARK_TALON,2,TownCountDone(uDRUID_TALON),1,10, 10)


    // TIER 3 UPGRADES
    //--------------------------------------------------------
      if tier >= 3 then
            if c_ally_total > 1 then
                  call BuildUpgr(1, uUPG_WELL_SPRING,50)
            else
                  call BuildUpgr(1, uUPG_WELL_SPRING,35)
            endif  
      endif
	call BuildAdvUpgr2(1, uUPG_MARKSMAN, 3,TownCountDone(uARCHER),0.5, 15,80)
	call BuildAdvUpgr2(1, uUPG_HARD_SKIN,3,TownCountDone(uMOUNTAIN_GIANT),0.25,15,80)
	call BuildAdvUpgr2(1, uUPG_RESIST_SKIN,3,TownCountDone(uMOUNTAIN_GIANT),0.25,15,60)
	call BuildAdvUpgr2(1, uUPG_CHIM_ACID,3, TownCountDone(uCHIMAERA),0.25,15,60)
	call BuildAdvUpgr2(1, uUPG_MARK_CLAW,3,TownCountDone(uDRUID_CLAW),1,10,80)

    // Harassing
    if not towerrush and not town_threatened then
      call Harass(1, HARASS_TARGET_PEONS, true, 4, 0.75, 0, GetPlayerRace(nearest_enemy) != RACE_UNDEAD , 50, 80)
      call Harass(3, HARASS_TARGET_PEONS, true, 4, 0.75, 0, GetPlayerRace(nearest_enemy) != RACE_UNDEAD , 50, 80)
      call Harass(4, HARASS_TARGET_PEONS, true, 4, 0.1, 0, GetPlayerRace(nearest_enemy) != RACE_UNDEAD and GetUnitCountDone(old_id[uDEMON_HUNTER]) > 0, 50, 80)
    endif




















































endfunction

