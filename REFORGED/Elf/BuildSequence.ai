      call BuildUnit(12, uARCHER, 50)
      if (8 - TownCountDone(uHIPPO_RIDER)) > 0 then
        call BuildUnit((8 - TownCountDone(uHIPPO_RIDER)), uHIPPO, 30)
      elseif TownCountDone(uHIPPO) < 4 then
        call BuildUnit(2, uHIPPO, 40)
      endif
      call BuildUnit(5, uCHIMAERA, 45)
      call BuildUnit(1, hero[1], 140)
      call BuildUnit(1, hero[2], 130)
      call BuildUnit(1, hero[3], 120)
      call BuildUpgr(1, uUPG_CHIM_ACID, 54)
      call BuildUpgr(1, uUPG_BOWS, 52)
      call BuildUpgr(1, uUPG_MARKSMAN, 52)




























elseif tier == 2 then

      call BuildUnit(4, uFAERIE_DRAGON, 40)
      call BuildUnit(6, uHIPPO_RIDER, 70)
