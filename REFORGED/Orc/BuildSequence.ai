function global_init_strategy takes nothing returns nothing
    call SetTierBlock(1, 0.75, 60, true)
    call SetTierBlock(2, 0.75, 90, true)
    if not IsMapFlagSet(MAP_RANDOM_HERO) then
      call AddBlock(1, uORC_ALTAR, false, 0, uBURROW, 16)
      call AddBlock(1, uORC_ALTAR, false, 0, uORC_BARRACKS, 16)
      call AddBlock(1, uORC_ALTAR, true, 0, uVOODOO_LOUNGE, 36)
    endif
    call AddBlock(1, uORC_BARRACKS, true, 0, uVOODOO_LOUNGE, 36)
    call AddBlock(1, uBURROW, true, 0, uVOODOO_LOUNGE, 36)
    call AddBlock(1, uORC_BARRACKS, false, 0, uFORGE, 20)
    call AddBlock(1, uORC_BARRACKS, true, 0, uORC_WATCH_TOWER, 36)
    call AddHarass(1, 1, uGRUNT)
    call AddHarass(3, 2, uWYVERN)
    call AddHarass(4, 1, uBLADE_MASTER)






















































endfunction
function global_build_sequence takes nothing returns nothing
    // Anti rush
    if GetRushFactor() < 5000 then
      call BuildAtMine(1, uORC_WATCH_TOWER, 42)
      call BuildFront(1, uORC_WATCH_TOWER, 45)
    endif
    if GetGold() > 500 then
      call BuildFront(2, uORC_WATCH_TOWER, 40)
    endif
    if HasExpanded() then
      call DefendTownsDone(1, uBURROW, 70)
      call DefendTownsFrontDone(2, uORC_WATCH_TOWER, 70)
      if GetGold() > 2000 then
            call DefendTownsFrontDone(4, uORC_WATCH_TOWER, 45)
            if tier >= 2 and TownCount(uORC_WATCH_TOWER) >= 4 and TownCount(uVOODOO_LOUNGE) < 2 then
                  call DefendTownsDone(1, uVOODOO_LOUNGE, 40)
            endif
      endif
    endif

    if urgent_healing_count > 3 then
          call BuildItem(1, uHEALING_SALVE, 120)
	  else
          call BuildItem(1, uHEALING_SALVE, 50)
    endif
    if urgent_healing_count > 2 then
      call BuildItem(2, uHEALING_SALVE, 50)
    endif
    call BuildItem(1, uM_SCROLL_OF_HEALING, 55)
    call BuildItem(1, uSCROLL_OF_SPEED, 40)
    call BuildItem(1, uPOTION_OF_LESSER_INVULNERBILITY, 50)
    call BuildItem(1, uLESSER_CLARITY_POTION, 35)
    call BuildItem(1, uM_TOWN_PORTAL, 100)
    if tier >= 2 then
      call BuildItem(1, uHEALING_POTION, 70)
      call BuildItem(1, uTOWN_PORTAL, 180)
      if c_ally_total > 0 then
            call BuildItem(1, uSTAFF_OF_PRESERVATION, 30)
      endif
    endif
    if tier >= 3 then
      if c_ally_total > 0 then
        call BuildItem(1, uSTAFF_OF_SANCTUARY, 30)
      endif
    endif
    if GetGold() > 1000 then
      call BuildItem(1, uSTAFF_OF_TELEPORTATION, 35)
    endif
    // DYNAMIC
    call ResetDynamicSystem()
    if tier == 1 then
      call AddUnitToAntiair(uHEAD_HUNTER,29)
      call AddUnitToAnticasters(uHEAD_HUNTER,29)
      call AddUnitToAntitowers(uCATAPULT,29)
      call AddUnitToAntipiercing(uGRUNT, 29)
      call AddUnitToAntinormal(uGRUNT, 30)
      call AddUnitToAntisiege(uGRUNT,29)
      call AddUnitToAntimagic(uHEAD_HUNTER,29)
      call AddUnitToAntiunarmored(uHEAD_HUNTER,30)
      call AddUnitToAntiunarmored(uCATAPULT,7)
      call AddUnitToAntilightarmor(uHEAD_HUNTER,1)
      call AddUnitToAntimediumarmor(uGRUNT,30)
      call AddUnitToAntiheavyarmor(uGRUNT,29)
      call DynamicBuildUnit(40, 40)
      call BuildUnit(1, merc_healer, 65)
      call BuildUnit(1, merc_stopper, 65)
    elseif tier == 2 then
      call AddUnitToAntiair(uHEAD_HUNTER,1)
      call AddUnitToAntiair(uBATRIDER,1)
      call AddUnitToAntiair(uRAIDER,GetUpgradeLevel(old_id[uUPG_ORC_ENSNARE]))
      call AddUnitToAnticasters(uSPIRIT_WALKER,15*GetUpgradeLevel(old_id[uUPG_ORC_SWALKER]))
      call AddUnitToAnticasters(uSHAMAN,5)
      call AddUnitToAntitowers(uCATAPULT,8+GetUpgradeLevel(old_id[uUPG_ORC_NAPTHA]))
      call AddUnitToAntitowers(uBATRIDER,1+GetUpgradeLevel(old_id[uUPG_ORC_FIRE]))
      call AddUnitToAntitowers(uRAIDER,1)
      call AddUnitToAntipiercing(uRAIDER, 1)
      call AddUnitToAntipiercing(uHEAD_HUNTER, 1)
      call AddUnitToAntinormal(uGRUNT, 19)
      call AddUnitToAntinormal(uKODO_BEAST, 1)
      call AddUnitToAntinormal(uTAUREN,10)
      call AddUnitToAntisiege(uHEAD_HUNTER,1)
      call AddUnitToAntisiege(uRAIDER,5)
      call AddUnitToAntisiege(uWYVERN,5)
      call AddUnitToAntimagic(uHEAD_HUNTER,1)
      call AddUnitToAntimagic(uRAIDER,5)
      call AddUnitToAntiunarmored(uHEAD_HUNTER,2)
      call AddUnitToAntiunarmored(uWYVERN,5)
      call AddUnitToAntiunarmored(uRAIDER,5)
      call AddUnitToAntilightarmor(uHEAD_HUNTER,3)
      call AddUnitToAntilightarmor(uWYVERN,1)
      call AddUnitToAntimediumarmor(uGRUNT,19)
      call AddUnitToAntimediumarmor(uKODO_BEAST,1)
      call AddUnitToAntiheavyarmor(uSHAMAN,15)
      call AddUnitToAntiheavyarmor(uWITCH_DOCTOR,2)
      call AddUnitToAntiheavyarmor(uSPIRIT_WALKER,3)
      call AddUnitToAntiheavyarmor(uWYVERN,5)
      call DynamicBuildUnit(64, 40)
      call BuildUnit(1, dragons[3], 65)
      call BuildUnit(1, merc_caster, 65)
    else
      call AddUnitToAntiair(uHEAD_HUNTER,1)
      call AddUnitToAntiair(uBATRIDER,1)
      call AddUnitToAntiair(uRAIDER,1)
      call AddUnitToAnticasters(uSPIRIT_WALKER,15)
      call AddUnitToAnticasters(uSHAMAN,5)
      call AddUnitToAntitowers(uCATAPULT,8+GetUpgradeLevel(old_id[uUPG_ORC_NAPTHA]))
      call AddUnitToAntitowers(uBATRIDER,1+GetUpgradeLevel(old_id[uUPG_ORC_FIRE]))
      call AddUnitToAntitowers(uRAIDER,1)
      call AddUnitToAntipiercing(uRAIDER, 1)
      call AddUnitToAntipiercing(uHEAD_HUNTER, 1)
      call AddUnitToAntinormal(uTAUREN, 19)
      call AddUnitToAntinormal(uKODO_BEAST, 1)
      call AddUnitToAntisiege(uHEAD_HUNTER,1)
      call AddUnitToAntisiege(uRAIDER,5)
      call AddUnitToAntisiege(uWYVERN,5)
      call AddUnitToAntimagic(uHEAD_HUNTER,1)
      call AddUnitToAntimagic(uRAIDER,5)
      call AddUnitToAntiunarmored(uHEAD_HUNTER,2)
      call AddUnitToAntiunarmored(uWYVERN,7)
      call AddUnitToAntiunarmored(uRAIDER,7)
      call AddUnitToAntilightarmor(uHEAD_HUNTER,1)
      call AddUnitToAntilightarmor(uWYVERN,1)
      call AddUnitToAntimediumarmor(uTAUREN,10)
      call AddUnitToAntimediumarmor(uKODO_BEAST,1)
      call AddUnitToAntiheavyarmor(uSHAMAN,15)
      call AddUnitToAntiheavyarmor(uWITCH_DOCTOR,2)
      call AddUnitToAntiheavyarmor(uSPIRIT_WALKER,3)
      call AddUnitToAntiheavyarmor(uWYVERN,5)
      call DynamicBuildUnit(100, 40)
      call BuildUnit(1, dragons[3], 65)
    endif

    // Global upgrades

    // TIER 1 UPGRADES
    //--------------------------------------------------------
    if tier >= 1 then
      call BuildUpgr(1, uUPG_BACKPACK_ORC, 10)
    endif
    call BuildAdvUpgr2(3, uUPG_ORC_MELEE, 1,TownCountDone(uGRUNT) + TownCountDone(uTAUREN) + TownCountDone(uRAIDER), 4,15,40)
    call BuildAdvUpgr2(3, uUPG_ORC_ARMOR, 1,TownCountDone(uGRUNT) + TownCountDone(uTAUREN) + TownCountDone(uRAIDER) + TownCountDone(uHEAD_HUNTER) + TownCountDone(uCATAPULT) + TownCountDone(uKODO_BEAST) + TownCountDone(uWYVERN) + TownCountDone(uBATRIDER), 3,15,40)
    call BuildAdvUpgr2(3, uUPG_ORC_RANGED, 1,TownCountDone(uHEAD_HUNTER) + TownCountDone(uCATAPULT) + TownCountDone(uKODO_BEAST) + TownCountDone(uWYVERN) + TownCountDone(uBATRIDER), 4,15,40)
    if not (c_enemy_total == c_enemy[R_ELF]) then
      call BuildAdvUpgr(2, uUPG_ORC_SPIKES, 1, 1, 1, 10, 10)
    else
      call BuildAdvUpgr(2, uUPG_ORC_SPIKES, 1, 1, 1, 5, 5)
    endif
    call BuildAdvUpgr2(1, uUPG_ORC_PILLAGE, 1,TownCountDone(uRAIDER) + TownCountDone(uGRUNT),0.5,15,60)

    // TIER 2 UPGRADES
    //--------------------------------------------------------
    if tier == 2 then
    endif


    call BuildAdvUpgr2(1, uUPG_ORC_ENSNARE, 2,TownCountDone(uRAIDER),0.33,15,80)
    call BuildAdvUpgr2(1, uUPG_ORC_REGEN, 2,TownCountDone(uHEAD_HUNTER)+TownCountDone(uBATRIDER),1,15,60)
    call BuildAdvUpgr2(2, uUPG_ORC_SHAMAN, 2,TownCountDone(uSHAMAN),0.33,15,90)
    call BuildAdvUpgr2(2, uUPG_ORC_DOCS, 2,TownCountDone(uWITCH_DOCTOR), 0.33,15,90)
    call BuildAdvUpgr2(2, uUPG_ORC_SWALKER, 2,TownCountDone(uSPIRIT_WALKER), 0.33,15,90)
    call BuildAdvUpgr2(2, uUPG_ORC_BERSERK, 2, TownCountDone(uGRUNT), 0.33,15,60)	  
    call BuildAdvUpgr(1, uUPG_ORC_BURROWS, 2,1,1,0,50)

    // TIER 3 UPGRADES
    //--------------------------------------------------------
    if tier == 3 then
    endif
    call BuildAdvUpgr2(1, uUPG_ORC_NAPTHA, 3,TownCountDone(uCATAPULT),0.25,15,60)
    call BuildAdvUpgr2(1, uUPG_ORC_WAR_DRUMS, 3,TownCountDone(uKODO_BEAST),0.25,10,60)
    call BuildAdvUpgr2(1, uUPG_ORC_BERSERKER, 3,TownCountDone(uHEAD_HUNTER), 0.33,15,75)
    call BuildAdvUpgr2(1, uUPG_ORC_VENOM, 3,TownCountDone(uWYVERN),0.33,15,85)
    call BuildAdvUpgr2(1, uUPG_ORC_PULVERIZE, 3,TownCountDone(uTAUREN),0.25,15,75)
    call BuildAdvUpgr2(1, uUPG_ORC_FIRE, 3,TownCountDone(uBATRIDER),0.33,15,75)


    // Global Mercenaries

    // Harass
    if not towerrush and not town_threatened then
      call Harass(1, HARASS_TARGET_PEONS, true, 4, 0.75, 0, GetPlayerRace(nearest_enemy) != RACE_UNDEAD , 40, 80)
      call Harass(3, HARASS_TARGET_PEONS, true, 5, 0.85, 0, GetPlayerRace(nearest_enemy) != RACE_UNDEAD , 120, 160)
      call Harass(4, HARASS_TARGET_PEONS, false, 12, 0.1, 0, hero[1] == uBLADE_MASTER and hero_skills[1] == HERO_SKILL_MINOR and GetPlayerRace(nearest_enemy) != RACE_UNDEAD, 36, 150)
    endif

























































endfunction

