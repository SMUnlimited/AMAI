function StrategyChanger takes nothing returns nothing
  local real array rp

  if not strat_change_allowed or strategy != chosen_strategy or debug_strategy > -1 or FoodUsed() >= 85 or GetMaximumEnemyStrength() < ver_strat_change_enemy_strength_min then
    return
  endif
  call chooseStrategyRP()
  call normalize_rp_strat()  // Makes priority much larger so the extra bonuses do not make as much of a difference


  set rp[STRAT_ROCNormalOrc] = rp[STRAT_ROCNormalOrc] + -8 * enemy_air
  set rp[STRAT_ROCFastSh] = rp[STRAT_ROCFastSh] + -8 * enemy_air
  set rp[STRAT_ROCKodo] = rp[STRAT_ROCKodo] + 0 * enemy_air
  set rp[STRAT_ROCWyvSh] = rp[STRAT_ROCWyvSh] + -1 * enemy_air
  set rp[STRAT_ROCHHunters] = rp[STRAT_ROCHHunters] + 1 * enemy_air
  set rp[STRAT_ROCRaiders] = rp[STRAT_ROCRaiders] + 1 * enemy_air
  set rp[STRAT_ROCCatapults] = rp[STRAT_ROCCatapults] + -2 * enemy_air
  set rp[STRAT_ROCHeadCatapults] = rp[STRAT_ROCHeadCatapults] + 1 * enemy_air
  set rp[STRAT_ROCNormalOrc] = rp[STRAT_ROCNormalOrc] + -1 * enemy_casters
  set rp[STRAT_ROCFastSh] = rp[STRAT_ROCFastSh] + -1 * enemy_casters
  set rp[STRAT_ROCKodo] = rp[STRAT_ROCKodo] + 0 * enemy_casters
  set rp[STRAT_ROCWyvSh] = rp[STRAT_ROCWyvSh] + 0 * enemy_casters
  set rp[STRAT_ROCHHunters] = rp[STRAT_ROCHHunters] + 1 * enemy_casters
