    call AddBlock(1, ELF_ALTAR, false, 0, MOON_WELL, 16)
    call AddBlock(1, ANCIENT_WAR, true, 0, ANCIENT_PROTECT, 36)
    call AddBlock(1, ANCIENT_WAR, false, 0, HUNTERS_HALL, 20)
    call AddBlock(1, ANCIENT_WAR, true, 0, DEN_OF_WONDERS, 36)
    call AddHarass(1, 2, HUNTRESS)
    call AddHarass(3, 2, FAERIE_DRAGON)




















































endfunction
function global_build_sequence takes nothing returns nothing
    // Anti rush
    if GetRushFactor() < 5000 or GetPlayerRace(nearest_enemy) == RACE_UNDEAD then
      call BuildFront(2, ANCIENT_PROTECT, 51)
    endif
    // home_location tower
    if GetGold() > 500 then
      call BuildFront(2, ANCIENT_PROTECT, 45)
    endif
    // exp tower
    if HasExpanded() then
      call DefendTownsFrontDone(2, ANCIENT_PROTECT, 75)
      if tier >= 3 and GetGold() > 2000 then
        call DefendTownsFrontDone(3, ANCIENT_PROTECT, 35)
        if TownCount(DEN_OF_WONDERS) < 2 then
          call DefendTownsDone(1, DEN_OF_WONDERS, 40)
        endif
        if TownCount(MOON_WELL) >= 8 and GetGold() > 2500 then
          call DefendTownsDone(2, MOON_WELL, 35)
        endif
      endif
    endif

    // transition train , sequence should be at the beginning
    if tier == 2 and racial_highbuild_lv2_total < 2 and GetGold() > 250 then
      call BuildUnit(1, HUNTRESS, 46)
      call BuildUnit(1, ARCHER, 40)
    elseif tier == 3 and racial_highbuild_lv3_total < 2 and GetGold() > 300 then
      call BuildUnit(1, DRYAD, 28+racial_highbuild_lv2_total*5)
      call BuildUnit(1, DRUID_CLAW, 24+racial_highbuild_lv2_total*5)
      call BuildUnit(1, HIPPO, 16+racial_highbuild_lv2_total*5)
      call BuildUnit(1, HIPPO_RIDER, 12+racial_highbuild_lv2_total*5)
    endif

    // Global Items
    call BuildItem(1, M_SCROLL_OF_HEALING, 40)
    //call BuildItem(1, SCROLL_OF_PROTECTION, 30)
    call BuildItem(1, POTION_OF_LESSER_INVULNERBILITY, 70)
    call BuildItem(1, MOONSTONE, 45)
    if tier == 1 then
      call BuildItem(1, M_TOWN_PORTAL, 100)
    endif
    if tier > 1 then
      call BuildItem(1, HEALING_POTION, 80)
      call BuildItem(1, TOWN_PORTAL, 180)
      call BuildItem(1, LESSER_CLARITY_POTION, 40)
      call BuildItem(1, STAFF_OF_PRESERVATION, 80)
    endif
    if hero[1] == WARDEN or hero[1] == DEMON_HUNTER then
      call BuildItem(1, BOOTS_OF_SPEED, 50)
    endif
    if tier >= 3 then
      if c_ally_total > 0 then
        call BuildItem(1, STAFF_OF_SANCTUARY, 30)
      endif
      //call BuildItem(1, ORB_OF_VENOM, 70)
      //call BuildItem(2, M_SCROLL_OF_HEALING, 30)
    endif
    if GetGold() > 1000 then
      call BuildItem(1, STAFF_OF_TELEPORTATION, 35)
    endif

    // DYNAMIC and Global Mercenaries
    call ResetDynamicSystem()
    if tier == 1 then
      call AddUnitToAntiair(ARCHER,29)
      call AddUnitToAnticasters(ARCHER,29)
      call AddUnitToAntitowers(BALLISTA,29)
      call AddUnitToAntipiercing(HUNTRESS, 15)
      call AddUnitToAntipiercing(ARCHER,15)
      call AddUnitToAntinormal(ARCHER,20)
      call AddUnitToAntinormal(HUNTRESS,10)
      call AddUnitToAntisiege(ARCHER,29)
      call AddUnitToAntimagic(ARCHER,29)
      call AddUnitToAntiunarmored(ARCHER,29)
      call AddUnitToAntilightarmor(ARCHER,29)
      call AddUnitToAntimediumarmor(HUNTRESS,29)
      call AddUnitToAntiheavyarmor(HUNTRESS,29)
      call DynamicBuildUnit(33, 38)
      call MercBuilder(merc_range,merc_stopper)
    elseif tier == 2 then
      call AddUnitToAntiair(ARCHER,15)
      call AddUnitToAntiair(DRYAD,10)
      call AddUnitToAntiair(HIPPO,10)
      call AddUnitToAntiair(DRUID_TALON,1)
      call AddUnitToAntiair(FAERIE_DRAGON,1)
      call AddUnitToAnticasters(FAERIE_DRAGON,4)
      //call AddUnitToAnticasters(MOUNTAIN_GIANT,1)  // need tier 3 UPG_RESIST_SKIN
      call AddUnitToAnticasters(DRYAD,8+GetUpgradeLevel(old_id[UPG_ABOLISH]))
      call AddUnitToAntitowers(BALLISTA,4)
      call AddUnitToAntitowers(MOUNTAIN_GIANT,1)
      call AddUnitToAntipiercing(DRUID_CLAW,4)
      call AddUnitToAntipiercing(ARCHER,3)
      call AddUnitToAntipiercing(MOUNTAIN_GIANT,2)
      call AddUnitToAntinormal(DRUID_CLAW,5)
      call AddUnitToAntinormal(DRUID_TALON, 3)
      call AddUnitToAntinormal(HUNTRESS,2)
      call AddUnitToAntisiege(ARCHER,1)
      call AddUnitToAntisiege(MOUNTAIN_GIANT,1)
      call AddUnitToAntimagic(MOUNTAIN_GIANT,1)
      call AddUnitToAntimagic(ARCHER,1)
      call AddUnitToAntiunarmored(DRYAD,7)
      call AddUnitToAntiunarmored(ARCHER,3)
      call AddUnitToAntiunarmored(HIPPO_RIDER,3)
      call AddUnitToAntiunarmored(BALLISTA,1)
      call AddUnitToAntilightarmor(DRYAD,4)
      call AddUnitToAntilightarmor(ARCHER,3)
      call AddUnitToAntilightarmor(HIPPO_RIDER,2)
      call AddUnitToAntilightarmor(DRUID_TALON,1)
      call AddUnitToAntimediumarmor(DRUID_CLAW,4)
      call AddUnitToAntimediumarmor(HUNTRESS,3)
      call AddUnitToAntiheavyarmor(DRUID_TALON,5)
      call DynamicBuildUnit(64, 40)
      call MercBuilder(merc_rush,merc_stopper)
    else
      call AddUnitToAntiair(HIPPO,10)
      call AddUnitToAntiair(DRYAD,8)
      call AddUnitToAntiair(ARCHER,3)
      call AddUnitToAntiair(DRUID_TALON,1)
      call AddUnitToAntiair(FAERIE_DRAGON,1)
      call AddUnitToAnticasters(FAERIE_DRAGON,3)
      call AddUnitToAnticasters(DRYAD,9)
      call AddUnitToAnticasters(MOUNTAIN_GIANT,2*GetUpgradeLevel(old_id[UPG_RESIST_SKIN]))
      call AddUnitToAntitowers(BALLISTA,5)
      call AddUnitToAntitowers(MOUNTAIN_GIANT,2)
      call AddUnitToAntitowers(CHIMAERA,3*GetUpgradeLevel(old_id[UPG_CHIM_ACID]))
      call AddUnitToAntipiercing(MOUNTAIN_GIANT,2)
      call AddUnitToAntipiercing(ARCHER,1)
      call AddUnitToAntinormal(DRUID_CLAW,5)
      call AddUnitToAntinormal(DRUID_TALON, 3)
      call AddUnitToAntinormal(CHIMAERA, 1)
      call AddUnitToAntinormal(HUNTRESS,2*GetUpgradeLevel(old_id[UPG_GLAIVE]))
      call AddUnitToAntisiege(DRUID_CLAW,2)
      call AddUnitToAntisiege(MOUNTAIN_GIANT,1)
      call AddUnitToAntimagic(MOUNTAIN_GIANT,2+GetUpgradeLevel(old_id[UPG_RESIST_SKIN]))
      call AddUnitToAntimagic(ARCHER,1)
      call AddUnitToAntiunarmored(DRYAD,4)
      call AddUnitToAntiunarmored(ARCHER,1)
      call AddUnitToAntiunarmored(HIPPO_RIDER,1)
      call AddUnitToAntiunarmored(BALLISTA,1)
      call AddUnitToAntilightarmor(DRYAD,7)
      call AddUnitToAntilightarmor(DRUID_TALON,1)
      call AddUnitToAntilightarmor(ARCHER,1)
      call AddUnitToAntilightarmor(HIPPO_RIDER,2)
      call AddUnitToAntilightarmor(CHIMAERA,4)
      call AddUnitToAntimediumarmor(DRUID_CLAW,8)
      call AddUnitToAntimediumarmor(HUNTRESS,1)
      call AddUnitToAntiheavyarmor(DRUID_TALON,5)
      call AddUnitToAntiheavyarmor(CHIMAERA,3)
      call DynamicBuildUnit(100, 40)
      call DragonAndMercBuilder()
    endif

    // GLOBAL UPGRADES
    call BuildAdvUpgr2(3, UPG_STR_MOON, 1,TownCountDone(ARCHER) + TownCountDone(HUNTRESS) + TownCountDone(HIPPO_RIDER),3,5,35)
    call BuildAdvUpgr2(3, UPG_MOON_ARMOR, 1,TownCountDone(ARCHER) + TownCountDone(HUNTRESS) + TownCountDone(HIPPO_RIDER),3,5,35)
    call BuildAdvUpgr2(3, UPG_STR_WILD, 1,TownCountDone(DRYAD) + TownCountDone(DRUID_CLAW) + TownCountDone(MOUNTAIN_GIANT) + TownCountDone(DRUID_TALON) + TownCountDone(CHIMAERA) + TownCountDone(FAERIE_DRAGON),3,5,35 )
    call BuildAdvUpgr2(3, UPG_HIDES, 1,TownCountDone(DRYAD) + TownCountDone(DRUID_CLAW) + TownCountDone(MOUNTAIN_GIANT) + TownCountDone(DRUID_TALON) + TownCountDone(CHIMAERA) + TownCountDone(FAERIE_DRAGON),3,5,35 )

    // TIER 1 UPGRADES
    //--------------------------------------------------------
    if tier >= 1 then
    endif
    call BuildAdvUpgr2(1, UPG_ULTRAVISION, 1, TownCountDone(ARCHER) + TownCountDone(HUNTRESS), 5, 3, 20)

    // TIER 2 UPGRADES
    //--------------------------------------------------------
    if tier >= 2 then
      call BuildUpgr(1, UPG_BLESSING, 25)
    endif
    if TownCountDone(ARCHER) > 1 and TownCountDone(HIPPO) > 1 then
      call BuildAdvUpgr2(1, UPG_HIPPO_TAME, 2,R2I(TownCountDone(ARCHER) + TownCountDone(HIPPO)/2),1,10,80)
    endif
    call BuildAdvUpgr2(1, UPG_BOWS,2,TownCountDone(ARCHER),1,10,40)
    call BuildAdvUpgr2(1, UPG_SCOUT, 2,TownCountDone(HUNTRESS),1,10,25)
    call BuildAdvUpgr2(1, UPG_BOLT, 2,TownCountDone(BALLISTA),0.25,10,40)
    call BuildAdvUpgr2(1, UPG_ABOLISH, 2, TownCountDone(DRYAD),0.33,15,80)
    call BuildAdvUpgr2(2, UPG_DRUID_CLAW, 2, TownCountDone(DRUID_CLAW),0.33,15,80)
    call BuildAdvUpgr2(2, UPG_DRUID_TALON, 2, TownCountDone(DRUID_TALON),0.33,15,80)

    // TIER 3 UPGRADES
    //--------------------------------------------------------
    if tier >= 3 then
      call BuildUpgr(1, UPG_WELL_SPRING,35 + c_ally_total / Max(c_ally_total, 1))
    endif
    call BuildAdvUpgr2(1, UPG_GLAIVE, 3,TownCountDone(HUNTRESS),0.5,15,70)
    call BuildAdvUpgr2(1, UPG_MARKSMAN, 3,TownCountDone(ARCHER),0.5,15,75)
    call BuildAdvUpgr2(1, UPG_HARD_SKIN,3,TownCountDone(MOUNTAIN_GIANT),0.25,15,75)
    call BuildAdvUpgr2(1, UPG_RESIST_SKIN,3,TownCountDone(MOUNTAIN_GIANT),0.25,15,50)
    call BuildAdvUpgr2(1, UPG_CHIM_ACID,3, TownCountDone(CHIMAERA),0.25,15,50)
    call BuildAdvUpgr2(1, UPG_MARK_CLAW,3,TownCountDone(DRUID_CLAW),1,10,70)
    call BuildAdvUpgr2(1, UPG_MARK_TALON,3,TownCountDone(DRUID_TALON),1,10, 10)


    // Harassing
    if not towerrush and not town_threatened then
      call Harass(1, HARASS_TARGET_PEONS, true, 4, 0.75, 0, GetPlayerRace(nearest_enemy) != RACE_UNDEAD , 50, 80)
      call Harass(3, HARASS_TARGET_PEONS, true, 4, 0.75, 0, GetPlayerRace(nearest_enemy) != RACE_UNDEAD , 50, 80)
    endif



















































endfunction

