    call AddBlock(1, HUMAN_ALTAR, false, 0, ARCANE_VAULT, 16)
    call AddBlock(1, HUMAN_ALTAR, false, 0, HOUSE, 16)
    call AddBlock(1, HUMAN_ALTAR, false, 0, BARRACKS, 16)
    call AddBlock(2, HOUSE, true, 0, ARCANE_VAULT, 36)
    call AddBlock(1, BARRACKS, false, 0, LUMBER_MILL, 20)
    call AddBlock(1, BARRACKS, true, 0, ARCANE_VAULT, 36)
    call AddBlock(1, BARRACKS, false, 0, BLACKSMITH, 20)
    call AddBlock(1, BLACKSMITH, false, 0, WATCH_TOWER, 36)
    call AddHarass(1, 2, FOOTMAN)
    call AddHarass(3, 2, GRYPHON)

























































endfunction
function global_build_sequence takes nothing returns nothing
    // Anti rush
    if GetRushFactor() < 5000 then
      call BuildUnit(2, ARCANE_TOWER, 60)
      call BuildAtMine(1, GUARD_TOWER, 45)
    endif
    // home_location tower
    if GetGold() > 550 then
      call BuildFront(1, GUARD_TOWER, 50)
      call BuildUnit(1, ARCANE_TOWER, 42)
      call BuildFront(1, ARCANE_TOWER, 45)
    endif
    // exp tower
    if HasExpanded() then
      call DefendTownsFrontDone(1, GUARD_TOWER, 75)
      call DefendTownsFrontDone(1, ARCANE_TOWER, 70)
      if tier >= 3 and GetGold() > 2000 then
        call DefendTownsFrontDone(2, GUARD_TOWER, 75)
        call DefendTownsFrontDone(1, ARCANE_TOWER, 70)
        call DefendTownsFrontDone(3, GUARD_TOWER, 35)
        call DefendTownsFrontDone(2, ARCANE_TOWER, 35)
        if TownCount(GUARD_TOWER) >= 4 and TownCount(ARCANE_VAULT) < 2 then
          call DefendTownsDone(1, ARCANE_VAULT, 40)
        endif
      endif
    endif
    // updata
    if TownCountDone(WATCH_TOWER) > 0 then
      if TownCount(WORKSHOP) > 0 then
        call BuildUnit(1, CANNON_TOWER, 40)
      else
        call BuildUnit(1, ARCANE_TOWER, 45)
        call BuildUnit(1, GUARD_TOWER, 32)
      endif
    endif

    // transition train , sequence should be at the beginning
    if tier == 2 and racial_highbuild_lv2_total < 2 and GetGold() > 250 then
      call BuildUnit(1, RIFLEMAN, 38)
      call BuildUnit(1, FOOTMAN, 30)
    elseif tier == 3 and racial_highbuild_lv3_total < 2 and GetGold() > 300 then
      call BuildUnit(1, HUMAN_DRAGON_HAWK, 34+racial_highbuild_lv2_total*5)
      call BuildUnit(1, SPELL_BREAKER, 24+racial_highbuild_lv2_total*5)
      call BuildUnit(1, COPTER, 15+racial_highbuild_lv2_total*5)
    endif

    // Global Items
    call BuildItem(1, SCROLL_OF_REGENERATION, 90)
    call BuildItem(1, M_SCROLL_OF_HEALING, 40)
    call BuildItem(1, POTION_OF_LESSER_INVULNERBILITY, 50)
    call BuildItem(1, LESSER_CLARITY_POTION, 35)
    call BuildItem(1, M_TOWN_PORTAL, 100)
    if tier > 1 then
      call BuildItem(1, HEALING_POTION, 80)
      call BuildItem(1, TOWN_PORTAL, 180)
      if c_ally_total > 0 then
        call BuildItem(1, STAFF_OF_PRESERVATION, 30)
      endif
    endif
    if tier >= 3 then
      call BuildItem(1, STAFF_OF_SANCTUARY, 80)
    endif
    if GetGold() > 1000 then
      call BuildItem(1, STAFF_OF_TELEPORTATION, 35)
    endif

	// DYNAMIC and Global Mercenaries
	call ResetDynamicSystem()
	if tier == 1 then
		call AddUnitToAntiair(RIFLEMAN,29)
		call AddUnitToAnticasters(RIFLEMAN,29)
		call AddUnitToAntitowers(FOOTMAN,29)
		call AddUnitToAntipiercing(FOOTMAN, 29)
		call AddUnitToAntinormal(FOOTMAN, 1)
		call AddUnitToAntisiege(RIFLEMAN,1)
		call AddUnitToAntisiege(FOOTMAN,1)
		call AddUnitToAntimagic(RIFLEMAN,29)
		call AddUnitToAntiunarmored(RIFLEMAN,29)
		call AddUnitToAntilightarmor(RIFLEMAN,29)
		call AddUnitToAntimediumarmor(FOOTMAN,1)
		call AddUnitToAntiheavyarmor(FOOTMAN,29)
		call DynamicBuildUnit(32, 40)
		call MercBuilder(merc_range,merc_healer)
	elseif tier == 2 then
		call AddUnitToAntiair(RIFLEMAN,5)
		call AddUnitToAntiair(COPTER,3+GetUpgradeLevel(old_id[UPG_FLAK]))
		call AddUnitToAntiair(HUMAN_DRAGON_HAWK,5)
		call AddUnitToAnticasters(SPELL_BREAKER,4+GetUpgradeLevel(old_id[UPG_CONT_MAGIC]))
		call AddUnitToAnticasters(PRIEST,6*GetUpgradeLevel(old_id[UPG_PRAYING]))
		call AddUnitToAntitowers(MORTAR,40)
		//call AddUnitToAntitowers(HUMAN_DRAGON_HAWK,10)  // need tier 3 UPG_CLOUD
		call AddUnitToAntipiercing(SPELL_BREAKER, 7)
		call AddUnitToAntipiercing(RIFLEMAN, 3)
		call AddUnitToAntipiercing(FOOTMAN, 2)
		call AddUnitToAntinormal(HUMAN_DRAGON_HAWK, 3)
		call AddUnitToAntinormal(FOOTMAN, 1)
		call AddUnitToAntisiege(RIFLEMAN,3)
		call AddUnitToAntisiege(SPELL_BREAKER,2)
		call AddUnitToAntisiege(FOOTMAN,1)
		call AddUnitToAntimagic(RIFLEMAN,1)
		call AddUnitToAntimagic(SPELL_BREAKER,3)
		call AddUnitToAntiunarmored(RIFLEMAN,6)
		call AddUnitToAntiunarmored(HUMAN_DRAGON_HAWK,3)
		call AddUnitToAntiunarmored(COPTER,1+GetUpgradeLevel(old_id[UPG_FLAK]))
		call AddUnitToAntiunarmored(MORTAR,2)
		call AddUnitToAntilightarmor(RIFLEMAN,25)
		call AddUnitToAntilightarmor(HUMAN_DRAGON_HAWK,25)
		call AddUnitToAntimediumarmor(SPELL_BREAKER,3)
		call AddUnitToAntimediumarmor(FOOTMAN,1)
		call AddUnitToAntiheavyarmor(SORCERESS,3)
		call AddUnitToAntiheavyarmor(PRIEST,2)
		call DynamicBuildUnit(64, 40)
		call BuildUnit(1, dragons[3], 65)
		call MercBuilder(merc_range,merc_rush)
	else
		call AddUnitToAntiair(RIFLEMAN,5)
		call AddUnitToAntiair(COPTER,3+GetUpgradeLevel(old_id[UPG_FLAK]))
		call AddUnitToAntiair(HUMAN_DRAGON_HAWK,5)
		call AddUnitToAntiair(GRYPHON,2)
		call AddUnitToAntiair(ROCKET_TANK,2*GetUpgradeLevel(old_id[UPG_TANK]))
		call AddUnitToAnticasters(SPELL_BREAKER,5)
		call AddUnitToAnticasters(PRIEST,11)
		call AddUnitToAntitowers(MORTAR,6)
		call AddUnitToAntitowers(HUMAN_DRAGON_HAWK,1)
		if GetUpgradeLevel(UPG_TANK) > 0 then
			call AddUnitToAntitowers(ROCKET_TANK,2)
		else
			call AddUnitToAntitowers(TANK,2)
		endif
		call AddUnitToAntitowers(COPTER,2*GetUpgradeLevel(old_id[UPG_BOMBS]))
		call AddUnitToAntipiercing(SPELL_BREAKER, 7)
		call AddUnitToAntipiercing(RIFLEMAN, 3)
		call AddUnitToAntipiercing(KNIGHT, 2)
		call AddUnitToAntinormal(KNIGHT, 9)
		call AddUnitToAntinormal(GRYPHON, 3)
		call AddUnitToAntinormal(HUMAN_DRAGON_HAWK, 2)
		call AddUnitToAntisiege(RIFLEMAN,3)
		call AddUnitToAntisiege(SPELL_BREAKER,2)
		call AddUnitToAntisiege(KNIGHT,1)
		call AddUnitToAntimagic(RIFLEMAN,1)
		call AddUnitToAntimagic(SPELL_BREAKER,3)
		call AddUnitToAntiunarmored(RIFLEMAN,6)
		call AddUnitToAntiunarmored(HUMAN_DRAGON_HAWK,2)
		call AddUnitToAntiunarmored(COPTER,1+GetUpgradeLevel(old_id[UPG_FLAK])+GetUpgradeLevel(old_id[UPG_BOMBS]))
		call AddUnitToAntiunarmored(MORTAR,2)
		call AddUnitToAntilightarmor(RIFLEMAN,10)
		call AddUnitToAntilightarmor(HUMAN_DRAGON_HAWK,10)
		call AddUnitToAntilightarmor(GRYPHON,4)
		call AddUnitToAntimediumarmor(KNIGHT,8)
		call AddUnitToAntimediumarmor(SPELL_BREAKER,3)
		//call AddUnitToAntimediumarmor(FOOTMAN,1)
		call AddUnitToAntiheavyarmor(GRYPHON,10)
		call AddUnitToAntiheavyarmor(SORCERESS,3)
		call AddUnitToAntiheavyarmor(PRIEST,2)
		call DynamicBuildUnit(100, 40)
		call DragonAndMercBuilder()
	endif

	// Global Upgrades
	call BuildAdvUpgr2(3, UPG_MELEE, 1,TownCountDone(FOOTMAN) + TownCountDone(KNIGHT) + TownCountDone(SPELL_BREAKER) + TownCountDone(HUMAN_DRAGON_HAWK) + TownCountDone(GRYPHON), 3,5,35)
	call BuildAdvUpgr2(3, UPG_ARMOR, 1,TownCountDone(FOOTMAN) + TownCountDone(KNIGHT) + TownCountDone(SPELL_BREAKER) + TownCountDone(COPTER) + TownCountDone(TANK), 3,5,35)
	call BuildAdvUpgr2(3, UPG_RANGED, 1,TownCountDone(RIFLEMAN) + TownCountDone(COPTER) + TownCountDone(TANK) + TownCountDone(MORTAR), 3,5,35)
	call BuildAdvUpgr2(3, UPG_LEATHER, 1,TownCountDone(RIFLEMAN) + TownCountDone(GRYPHON) + TownCountDone(HUMAN_DRAGON_HAWK) + TownCountDone(MORTAR), 3,5,35)
	call BuildAdvUpgr(3, UPG_MASONRY,1,1,1,5,20)

	// TIER 1 UPGRADES
	//--------------------------------------------------------
	if tier == 1 then
	endif
	call BuildAdvUpgr2(1, UPG_DEFEND, 1,TownCountDone(FOOTMAN), 0.33, 15, 60)

	// TIER 2 UPGRADES
	//--------------------------------------------------------
	if tier >= 2 then
	endif
	call BuildAdvUpgr(2, UPG_WOOD,2,1,1,5,40)
	call BuildAdvUpgr2(1, UPG_SENTINEL, 2, TownCountDone(WATCH_TOWER) + c_enemy[R_ELF],2, 5, 25)
	call BuildAdvUpgr2(1, UPG_GUN_RANGE, 2,TownCountDone(RIFLEMAN),1,10, 40)
	call BuildAdvUpgr2(1, UPG_SIGHT, 2,TownCountDone(MORTAR),1,5, 25)
	call BuildAdvUpgr2(2, UPG_PRAYING, 2,TownCountDone(PRIEST), 0.33, 15, 80)
	call BuildAdvUpgr2(2, UPG_SORCERY,2,TownCountDone(SORCERESS), 0.33, 15, 80)

	// TIER 3 UPGRADES
	//--------------------------------------------------------
	if tier == 3 then
		call BuildUpgr(1, UPG_SUN_BLADE, 40)
	endif
	call BuildAdvUpgr2(1, UPG_FLAK, 3,TownCountDone(COPTER),0.33,15, 40)
	call BuildAdvUpgr2(1, UPG_BOMBS, 3,TownCountDone(COPTER),0.2,35, 40)
	call BuildAdvUpgr2(1, UPG_FRAGS, 3,TownCountDone(MORTAR), 0.33,15, 40)
	call BuildAdvUpgr2(1, UPG_CONT_MAGIC, 3,TownCountDone(SPELL_BREAKER), 0.5, 10, 50)
	call BuildAdvUpgr2(1, UPG_CLOUD, 3, TownCountDone(HUMAN_DRAGON_HAWK),0.33,15, 70)
	call BuildAdvUpgr2(1, UPG_HAMMERS, 3,TownCountDone(GRYPHON), 0.33,15, 60)
	call BuildAdvUpgr2(1, UPG_BREEDING, 3,TownCountDone(KNIGHT) + TownCountDone(GRYPHON) + TownCountDone(HUMAN_DRAGON_HAWK),1,15, 60)
	call BuildAdvUpgr2(1, UPG_TANK, 3, TownCountDone(TANK),0.25,15, 50)


	// Harass
	if not towerrush and not town_threatened then
		call Harass(1, HARASS_TARGET_PEONS, true, 5, 0.75, 0, GetPlayerRace(nearest_enemy) != RACE_UNDEAD , 50, 80)
		call Harass(3, HARASS_TARGET_PEONS, true, 5, 0.75, 0, GetPlayerRace(nearest_enemy) != RACE_UNDEAD , 80, 100)
		if GetUnitCountDone(old_id[PALADIN]) > 0 then
			call Harass(4, HARASS_TARGET_PEONS, true, 4, 0.75, 0, GetPlayerRace(nearest_enemy) == RACE_UNDEAD , 50, 80)
		endif
	endif






























































endfunction

